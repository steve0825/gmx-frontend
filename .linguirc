{
  "locales": [
    "en",
    "es",
    "ko",
    "ja",
    "zh",
    "ru",
    "fr",
    "de",
    "pseudo"
  ],
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "<rootDir>/src/locales/{locale}/messages",
      "include": [
        "<rootDir>"
      ],
      "exclude": [
        "**/node_modules/**"
      ]
    }
  ],
  "formatOptions": {
    "lineNumbers": false
  },
  "fallbackLocales": {
    "default": "en"
  },
  "format": "po",
  "orderBy": "messageId",
  "pseudoLocale": "pseudo"
}
